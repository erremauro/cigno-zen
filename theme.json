{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"padding": true,
			"blockGap": "2em"
		},
		"color": {
			"background": true,
			"link": true,
			"text": true,
			"palette": [
				{
					"name": "Base",
					"color": "#141414",
					"slug": "base"
				},
				{
					"name": "Contrast",
					"color": "#96969B",
					"slug": "contrast"
				},
				{
					"name": "Accent",
					"color": "#439cd6",
					"slug": "accent"
				}
			]
		},
		"typography": {
			"writingMode": true,
			"fluid": true,
			"fontFamilies": [
				{
					"name": "LibreBaskervville",
					"slug": "libre-baskervville",
					"fontFamily": "LibreBaskervville, serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/libre-baskervville/LibreBaskerville-Regular.ttf"
							],
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontFamily": "LibreBaskervville"
						},
						{
							"src": [
								"file:./assets/fonts/libre-baskervville/LibreBaskerville-Italic.ttf"
							],
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontFamily": "LibreBaskerville"
						},
						{
							"src": [
								"file:./assets/fonts/libre-baskervville/LibreBaskerville-Bold.ttf"
							],
							"fontWeight": "700",
							"fontStyle": "bold",
							"fontFamily": "LibreBaskerville"
						}
					]
				}
			]
		}
	},
	"styles": {

	},
	"templateParts": [

	]
}
